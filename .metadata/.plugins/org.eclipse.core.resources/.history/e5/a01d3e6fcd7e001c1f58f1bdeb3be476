package Ex5;

import java.util.Scanner;

public class Ex5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner inp = new Scanner(System.in); 
		double a, b ,c ,med;
		System.out.print("Espera as três notas tiradas em ordem e respectivamente: ");
		med= ((a*2)+(b*3)+(c*5))/10
				
		System.out.print("Sua média ponderada é de: "+med+".");
		inp.close();
	}

}
